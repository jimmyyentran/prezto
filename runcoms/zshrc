# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...

# enable ctrl
bindkey -e

# EDITOR=vim
alias v='$EDITOR'
alias zrc='~/.dotfiles/zsh/'
alias zr='$EDITOR ~/.zshrc'
alias vr='$EDITOR ~/.vimrc'
alias lt='ls -lAhtr'
alias lz='ls -lAS'
alias gno='gnome-open'
alias q="exit"
alias dl="cd ~/Downloads"
alias dt="cd ~/Desktop"
alias doc="cd ~/Documents"
alias make='nocorrect make' #stop make from autocorrecting things
alias szsh="source ~/.zshrc" # source zsh
alias python=python3
# export PYTHONPATH=$PYTHONPATH:/home/jt2/Workspace/python/gdata-python-client/src
